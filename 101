import java.util.HashSet;
import java.util.Set;

public class TilePossibilities {
    public static void main(String[] args) {
        String tiles = "AAB";
        System.out.println(numTilePossibilities(tiles));  // Output: 8
    }

    public static int numTilePossibilities(String tiles) {
        Set<String> result = new HashSet<>();
        boolean[] used = new boolean[tiles.length()];
        backtrack("", tiles, used, result);
        return result.size();
    }

    private static void backtrack(String path, String tiles, boolean[] used, Set<String> result) {
        if (!path.isEmpty()) {
            result.add(path);
        }
        for (int i = 0; i < tiles.length(); i++) {
            if (used[i]) continue;
            used[i] = true;
            backtrack(path + tiles.charAt(i), tiles, used, result);
            used[i] = false;
        }
    }
}
